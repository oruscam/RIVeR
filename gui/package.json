{
  "name": "river-gui",
  "productName": "RIVeR 3.0.0",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && vite build",
    "lint": "eslint . --c ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "install-pyinstaller": "pip install pyinstaller",
    "build-cli": "pyinstaller -F --distpath . -n river-cli ../river/cli/__main__.py",
    "build-cli-onedir": "pyinstaller -D --distpath . -n river-cli --contents-directory . ../river/cli/__main__.py",
    "build:win:one-dir": "electron-builder --config electron-builder.onedir.json5 --win",
    "build:win:one-file": "electron-builder --config electron-builder.onefile.json5 --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux",
    "clean": "rimraf dist dist-electron"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.4",
    "@theme-toggles/react": "^4.1.0",
    "d3": "^7.9.0",
    "d3-fetch": "^3.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "i18next": "^23.11.4",
    "konva": "^9.3.11",
    "react": "^18.2.0",
    "react-data-grid": "^7.0.0-beta.46",
    "react-datepicker": "^7.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.4",
    "react-i18next": "^14.1.1",
    "react-icons": "^5.2.1",
    "react-konva": "^18.2.10",
    "react-redux": "^9.1.2",
    "react-svg": "^16.1.34",
    "react-use-wizard": "^2.3.0",
    "react-window": "^1.8.10",
    "tree-kill": "^1.2.2",
    "use-image": "^1.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-slick": "^0.23.13",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^32.0.2",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "python-shell": "^5.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "rimraf": "^5.0.0"
  },
  "main": "dist-electron/main.js"
}
